---
import { Icons } from './Icons/Icons';
import { parseComponentWithoutAstro } from '@selleo/core';
import { Icon } from '@selleo/core/src/Icon';

const { iconsData } = Astro.props;

const iconsList = iconsData.map(({ name }: { name: string }) => {
  const { HTMLString, ReactString } = parseComponentWithoutAstro(name, Icon);

  return {
    name,
    htmlString: HTMLString,
    reactString: ReactString,
  };
});
---

<Icons iconsList={iconsList} client:load />
