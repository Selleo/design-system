---
import { Icons } from "./Icons/Icons";
import { parseComponentWithoutAstro } from "@selleo/core/src/parser";

const { iconsData } = Astro.props;

const iconsList = iconsData.map(({ name, component: Component }: any) => {
  const { HTMLString, ReactString } = parseComponentWithoutAstro(Component);

  console.log({ HTMLString, ReactString });

  return {
    name,
    component: Component,
    htmlString: HTMLString,
    reactString: ReactString,
  };
});
---

<Icons iconsList={iconsList} client:load />
