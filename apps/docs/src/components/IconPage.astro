---
import render from 'preact-render-to-string';

import { Icons } from './Icons/Icons';
import { parseToReact } from '@selleo/core/src/parser';
import Modal from './Icons/Modal.astro';

const { iconsData } = Astro.props;

const iconsList = iconsData.map(({ name, component }: any) => {
  const htmlString = render(component);

  return {
    name,
    component,
    htmlString,
    reactString: parseToReact(htmlString),
  };
});
---

<Icons iconsList={iconsList} client:load />
